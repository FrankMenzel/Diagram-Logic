                <td id="test-elem32"> </td>
                <td id="test-elem33"> </td>
              </tr>
            </tbody>
          </table>
         
          <div id="nav-buttons">
            <button class="nav" id="nextcase-btn" onclick="nextCase()"><h3>Start Test</h3></button>
            <button class="nav" id="reset-btn" onclick="resetCase()"><h3>Reset</h3></button>
          </div>
        </div>
</div>
