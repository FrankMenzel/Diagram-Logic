
<div>
  <h2><span>Test: </span> <span id="test-name">{{testName}}</span> <span style="display:none"> Id: {{this._id}}</span></h2>
</div>   
<div>
  <h3 id="current-case">Case: none</h3>
</div>   
<div>
  <h3><span>Complexity: </span><span id="complexity">{{complexity}}</span></h3>
</div>  

<ul id = "testcases" style="display:none">
  {{#each cases}}      
    <li><span id="case{{@index}}">{{this._id}}</span>  <span id="result{{@index}}">Result: </span> </li>    
  {{/each}}
</ul>  

<div style="display:none">
  <h6><span> Number of cases: </span> <span id="num-cases">{{numOfCases}}</span> </h6>
</div>  

    

<div id ="test-and-loader">
        <div id="loader-icon"><img  src="../images/loader.gif" alt="" width="310"></img> </div>
        <div id="test-container">
